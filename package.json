{
  "name": "code-sorting",
  "version": "1.0.3",
  "description": "Code sorting algorithms!",
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "author": "Yusuke Hayashi <yusuke8h@gmail.com>",
  "main": "dev-server.js",
  "scripts": {
    "build": "webpack --progress --colors --mode='production'",
    "fix": "rm -rf node_modules package-lock.json && npm run update && npm run sort && npm i && npm run fmt && npm run lint",
    "fmt": "prettier --write src",
    "lint": "eslint --fix --ext .ts src",
    "serve": "nodemon dev-server.js",
    "sort": "npx sort-package-json",
    "start": "webpack --watch --progress --colors",
    "update": "npx npm-check-updates -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run fmt && npm run lint && npm run build"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "brace": "^0.11.1",
    "d3-selection": "^1.4.1"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "serve-favicon": "^2.5.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
